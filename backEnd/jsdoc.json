{
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "excludePattern": "(node_modules/|docs)",
    "excludePattern": "(^|\\/|\\\\/)_",
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
    "opts": {
        "template": "templates/default",  
        "encoding": "utf8",              
        "destination": "./out/",         
        "recurse": true,     
        "tutorials": ""
    }
}

